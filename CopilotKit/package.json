{
  "name": "copilotkit",
  "private": false,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --concurrency 14",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "test": "turbo test",
    "docs": "ts-node ./scripts/docs/gen.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-prettier": "prettier --check \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build lint && changeset version && changeset publish",
    "freshbuild": "pnpm -w clean && pnpm i && pnpm -w build",
    "precommit": "pnpm run docs"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "eslint": "^8.56.0",
    "eslint-config-custom": "workspace:*",
    "glob": "^10.3.12",
    "install": "^0.13.0",
    "npm": "^10.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.6",
    "typescript": "^5.2.3"
  },
  "packageManager": "pnpm@9.5.0",
  "dependencies": {
    "@changesets/cli": "^2.26.2"
  },
  "keywords": [
    "copilotkit",
    "copilot",
    "react",
    "nextjs",
    "nodejs",
    "ai",
    "assistant",
    "javascript",
    "automation",
    "textarea"
  ],
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.3.4",
      "@types/react-dom": "18.3.0",
      "typescript": "5.4.5"
    }
  }
}
